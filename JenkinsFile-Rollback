node{
    properties([
        pipelineTriggers([
           [$class: 'GenericTrigger',
            genericVariables: [
             [
              key: 'region',
              value: '$.region',
              expressionType: 'JSONPath'
             ]
             ]

             token = 'rollback-deployment'
             ]
        ])
    ])

    stage("Roll back"){
        echo "Rolling Back in $region region"
    }
}